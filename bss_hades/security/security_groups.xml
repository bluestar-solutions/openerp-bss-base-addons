<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	<record model="ir.module.category" id="module_category_hades">
            <field name="name">HADES</field>
            <field name="sequence" eval="400" />
        </record>
        
        <record id="group_hades_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_hades" />
            <field name="share" eval="0" />
            <field name="sequence" eval="100" />
        </record>
        
        <record id="group_hades_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_hades" />
            <field name="implied_ids" eval="[(6,0,[ref('group_hades_user')])]"/>
            <field name="share" eval="0" />
            <field name="sequence" eval="101" />
        </record>
	</data>
</openerp>